{
  "name": "rusty-core",
  "version": "3.0.3",
  "description": "Option and Result as inspired by https://doc.rust-lang.org/stable/core",
  "main": "lib/lib.js",
  "type": "commonjs",
  "directories": {
    "test": "test",
    "load_url": "ts-node cli/load_url.ts"
  },
  "scripts": {
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HaKr/rusty-core.git"
  },
  "keywords": [
    "Rust",
    "Result",
    "Option",
    "Pointer safety"
  ],
  "author": "Harry de Kroon",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/HaKr/rusty-core/issues"
  },
  "homepage": "https://github.com/HaKr/rusty-core#readme",
  "mocha": {
    "require": "ts-node/register",
    "spec": "./test/*.ts",
    "reporter": "dot",
    "extension": [
      "ts"
    ]
  },
  "devDependencies": {
    "@types/mocha": "10.0.1",
    "@types/node": "18.11.18",
    "mocha": "10.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  }
}
