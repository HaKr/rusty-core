{
	"compilerOptions": {
		"outDir": "lib",
		"declaration": true,
		"target": "es6",
		"lib": ["ES2022"],
		"module": "ES2022",
		"moduleResolution": "Node",
		"esModuleInterop":true,
		"strict": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"forceConsistentCasingInFileNames": true,
		"sourceMap": true,
	},
   "include": ["src","test"],
	"ts-node": {
		"emit": true
	}
}
